{
  "name": "react-router-demo",
  "version": "1.0.0",
  "author": "Alex Sears <me@alexsears.com> (http://alexsears.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/searsaw/react-router-demo.git"
  },
  "engines": {
    "node": "6.10.3"
  },
  "scripts": {
    "clear": "rimraf -rf dist",
    "lint": "eslint server.js \"src/**/*.js\"",
    "prebuild": "npm run clear",
    "build:server": "babel -d dist/server ./src/server/",
    "build:client": "webpack",
    "build": "npm run build:client && npm run build:server",
    "dev": "nodemon --ignore dist/ --ignore src/ --exec \"npm run build:server && node dist/server/index.js\"",
    "prestart": "npm run build",
    "start": "node dist/server/index.js"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "copy": "^0.3.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.17.1",
    "connect-history-api-fallback": "^1.3.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "lodash.times": "^4.3.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "next",
    "react-router-dom": "^4.1.1",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.68.3",
    "store": "^2.0.12",
    "webpack": "^2.6.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
